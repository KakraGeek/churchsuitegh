{
  "name": "church-suite-gh",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Mobile-first PWA for Ghanaian churches - Member management, MoMo giving, attendance tracking",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:smoke": "vitest run --reporter=verbose --config vitest.smoke.config.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed-members.ts",
    "db:seed-test-users": "tsx scripts/seed-test-users.ts",
    "db:verify-test-users": "tsx scripts/verify-test-users.ts",
    "db:fix-test-user-ids": "tsx scripts/fix-test-user-ids.ts",
    "db:check-all-members": "tsx scripts/check-all-members.ts",
    "db:fix-mockadmin-only": "tsx scripts/fix-mockadmin-only.ts",
    "db:seed-events": "tsx scripts/seed-events.ts",
    "db:seed-inventory": "tsx scripts/seed-inventory.ts",
    "db:seed-sunday-service": "tsx scripts/seed-sunday-service.ts"
  },
  "dependencies": {
    "@clerk/clerk-react": "^4.30.0",
    "@neondatabase/serverless": "^0.7.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.29.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.294.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^2.0.0",
    "vercel": "^46.0.2",
    "workbox-window": "^7.3.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jspdf": "^1.3.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.6",
    "drizzle-zod": "^0.5.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.1",
    "playwright": "^1.40.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "~5.5.4",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^1.0.4"
  }
}
